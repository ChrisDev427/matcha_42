{"ast":null,"code":"import { useI18n } from \"vue-i18n\";\nimport { ref, watch } from 'vue';\nexport default {\n  name: \"RegisterForm\",\n  setup() {\n    // Traduction ----------------------------------\n    const {\n      t\n    } = useI18n();\n    const registerTitle = t(\"registerTitle\");\n    const registerSubTitle = t(\"registerSubTitle\");\n    const userName = t(\"userName\");\n    const firstName = t(\"firstName\");\n    const lastName = t(\"lastName\");\n    const password = t(\"password\");\n    const passwordConfirm = t(\"passwordConfirm\");\n    const send = t(\"send\");\n    // Input Object --------------------------------\n    let inputs = ref({\n      userName: '',\n      userTooLong: false,\n      firstName: '',\n      firstTooLong: false,\n      lastName: '',\n      lastTooLong: false,\n      email: '',\n      password: '',\n      repeatPassword: '',\n      samePassword: false\n    });\n    watch(inputs, newValue => {\n      if (newValue.userName.length > 10) {\n        inputs.value.userTooLong = true;\n      } else {\n        inputs.value.userTooLong = false;\n      }\n      if (newValue.firstName.length > 10) {\n        inputs.value.firstTooLong = true;\n      } else {\n        inputs.value.firstTooLong = false;\n      }\n      if (newValue.lastName.length > 10) {\n        inputs.value.lastTooLong = true;\n      } else {\n        inputs.value.lastTooLong = false;\n      }\n      if (newValue.repeatPassword != newValue.password) {\n        inputs.value.samePassword = false;\n      } else {\n        inputs.value.samePassword = true;\n      }\n    }, {\n      deep: true\n    });\n    return {\n      registerTitle,\n      registerSubTitle,\n      userName,\n      firstName,\n      lastName,\n      password,\n      passwordConfirm,\n      send,\n      inputs\n    };\n  }\n};","map":{"version":3,"names":["useI18n","ref","watch","name","setup","t","registerTitle","registerSubTitle","userName","firstName","lastName","password","passwordConfirm","send","inputs","userTooLong","firstTooLong","lastTooLong","email","repeatPassword","samePassword","newValue","length","value","deep"],"sources":["/Users/chris/Desktop/matcha_42/frontend/src/components/forms/RegisterForm.vue"],"sourcesContent":["<template>\n    <div class=\"register--container \">\n        <h3>{{ $t('registerTitle') }}</h3>\n        <h3 id=\"sub--title\">{{ $t('registerSubTitle') }}</h3>\n        <form action=\"\" class=\"register--form\">\n            <input :class=\"{ 'text-red': inputs.userTooLong }\" v-model=\"inputs.userName\" type=\"text\" :placeholder=\"$t('userName')\">\n            <input :class=\"{ 'text-red': inputs.firstTooLong }\" v-model=\"inputs.firstName\" type=\"text\" :placeholder=\"$t('firstName')\">\n            <input :class=\"{ 'text-red': inputs.lastTooLong }\" v-model=\"inputs.lastName\" type=\"text\" :placeholder=\"$t('lastName')\">\n            <input v-model=\"inputs.email\" type=\"email\" placeholder=\"e-mail\">\n            <input v-memo=\"inputs.password\" type=\"password\" :placeholder=\"$t('password')\">\n            <input :class=\"{ 'text-red': !inputs.samePassword }\" v-memo=\"inputs.repeatPassword\" type=\"password\" :placeholder=\"$t('passwordConfirm')\">\n            <div class=\"submit--btn\">\n                <button>{{ $t('send') }}</button>\n            </div>\n        </form>\n    </div>\n</template>\n\n<script>\nimport { useI18n } from \"vue-i18n\";\nimport{ ref, watch } from 'vue';\n\n\nexport default {\n    name: \"RegisterForm\",\n\n    setup() {\n        // Traduction ----------------------------------\n        const { t } = useI18n();\n        const registerTitle = t(\"registerTitle\");\n        const registerSubTitle = t(\"registerSubTitle\");\n        const userName = t(\"userName\");\n        const firstName = t(\"firstName\");\n        const lastName = t(\"lastName\");\n        const password = t(\"password\");\n        const passwordConfirm = t(\"passwordConfirm\");\n        const send = t(\"send\");\n        // Input Object --------------------------------\n        let inputs = ref({\n            userName: '',\n            userTooLong: false,\n            firstName: '',\n            firstTooLong: false,\n            lastName: '',\n            lastTooLong: false,\n            email: '',\n            password: '',\n            repeatPassword: '',\n            samePassword: false,\n        });\n\n        watch(inputs, (newValue) => {\n           \n            if (newValue.userName.length > 10) {\n                inputs.value.userTooLong = true;\n            } else {\n                inputs.value.userTooLong = false;\n            }\n            if (newValue.firstName.length > 10) {\n                inputs.value.firstTooLong = true;\n            } else {\n                inputs.value.firstTooLong = false;\n            }\n            if (newValue.lastName.length > 10) {\n                inputs.value.lastTooLong = true;\n            } else {\n                inputs.value.lastTooLong = false;\n            }\n\n            if (newValue.repeatPassword != newValue.password) {\n                inputs.value.samePassword = false;\n            } else {\n                inputs.value.samePassword = true;\n            }\n\n        }, { deep: true });\n\n\n        return { \n            registerTitle, \n            registerSubTitle,\n            userName,\n            firstName,\n            lastName,\n            password,\n            passwordConfirm,\n            send,\n            inputs,\n         };\n    },\n}\n</script>\n\n<style lang=\"scss\">\n.register--container {\n    // border: solid 1px red;\n\n    // border: solid 1px red;\n    width: auto;\n    height: auto;\n    padding: 10px 35px 45px 35px;\n    border-radius: 15px;\n    background-image: linear-gradient(to right,\n            #ff24a7af,\n            #8890feb2);\n    box-shadow: 0 0 28px rgba(0, 0, 0, 0.498);\n    cursor: default;\n    user-select: none;\n\n    h3 {\n\n        margin: 10px 0 0 0;\n        color: white;\n        text-align: center;\n        font-weight: 900;\n        text-transform: capitalize;\n        font-size: calc(min(3vw + 2vh, 70px));\n        text-shadow: 0 0 18px rgba(255, 255, 255, 0.438);\n\n    }\n\n    #sub--title {\n        font-weight: 600;\n        font-size: calc(min(2vw + 1.6vh, 50px));\n        margin-bottom: 10px;\n    }\n\n    .register--form {\n        display: grid;\n        justify-items: center;\n\n        input {\n            padding: 6px;\n            margin: 10px;\n            width: 100%;\n            border: none;\n            outline: none;\n            border-radius: 8px;\n            text-align: center;\n            font-weight: 600;\n            background-color: var(--purple-placeholder-bg);\n            color: var(--purple);\n\n        }\n\n        input::placeholder {\n            color: white;\n            font-weight: 400;\n\n            /* Couleur du placeholder */\n        }\n\n        .submit--btn {\n            margin-top: 25px;\n\n            button {\n\n                // padding: 10px 30px 10px 30px;\n                height: 45px;\n                width: 180px;\n                border-radius: 10px;\n                border: none;\n                color: white;\n                font-weight: 600;\n                font-size: 1.2rem;\n\n                transition: all 0.4s;\n                cursor: pointer;\n                box-shadow: 0 0 6px rgba(0, 0, 0, 0.381);\n\n                background-image: linear-gradient(to right,\n                        #ff24a7af,\n                        #8890feb2);\n\n                &:hover {\n                    background-image: linear-gradient(to right,\n                            #ff24a78a,\n                            #8890fe90);\n                    /* Dégradé de couleur */\n                    color: var(--purple);\n                    transform: scale(1.1);\n                }\n            }\n\n        }\n\n        @media (min-width: 800px) {\n            width: 70%;\n            margin: auto;\n\n        }\n    }\n}\n</style>"],"mappings":"AAmBA,SAASA,OAAM,QAAS,UAAU;AAClC,SAAQC,GAAG,EAAEC,KAAI,QAAS,KAAK;AAG/B,eAAe;EACXC,IAAI,EAAE,cAAc;EAEpBC,KAAKA,CAAA,EAAG;IACJ;IACA,MAAM;MAAEC;IAAE,IAAIL,OAAO,CAAC,CAAC;IACvB,MAAMM,aAAY,GAAID,CAAC,CAAC,eAAe,CAAC;IACxC,MAAME,gBAAe,GAAIF,CAAC,CAAC,kBAAkB,CAAC;IAC9C,MAAMG,QAAO,GAAIH,CAAC,CAAC,UAAU,CAAC;IAC9B,MAAMI,SAAQ,GAAIJ,CAAC,CAAC,WAAW,CAAC;IAChC,MAAMK,QAAO,GAAIL,CAAC,CAAC,UAAU,CAAC;IAC9B,MAAMM,QAAO,GAAIN,CAAC,CAAC,UAAU,CAAC;IAC9B,MAAMO,eAAc,GAAIP,CAAC,CAAC,iBAAiB,CAAC;IAC5C,MAAMQ,IAAG,GAAIR,CAAC,CAAC,MAAM,CAAC;IACtB;IACA,IAAIS,MAAK,GAAIb,GAAG,CAAC;MACbO,QAAQ,EAAE,EAAE;MACZO,WAAW,EAAE,KAAK;MAClBN,SAAS,EAAE,EAAE;MACbO,YAAY,EAAE,KAAK;MACnBN,QAAQ,EAAE,EAAE;MACZO,WAAW,EAAE,KAAK;MAClBC,KAAK,EAAE,EAAE;MACTP,QAAQ,EAAE,EAAE;MACZQ,cAAc,EAAE,EAAE;MAClBC,YAAY,EAAE;IAClB,CAAC,CAAC;IAEFlB,KAAK,CAACY,MAAM,EAAGO,QAAQ,IAAK;MAExB,IAAIA,QAAQ,CAACb,QAAQ,CAACc,MAAK,GAAI,EAAE,EAAE;QAC/BR,MAAM,CAACS,KAAK,CAACR,WAAU,GAAI,IAAI;MACnC,OAAO;QACHD,MAAM,CAACS,KAAK,CAACR,WAAU,GAAI,KAAK;MACpC;MACA,IAAIM,QAAQ,CAACZ,SAAS,CAACa,MAAK,GAAI,EAAE,EAAE;QAChCR,MAAM,CAACS,KAAK,CAACP,YAAW,GAAI,IAAI;MACpC,OAAO;QACHF,MAAM,CAACS,KAAK,CAACP,YAAW,GAAI,KAAK;MACrC;MACA,IAAIK,QAAQ,CAACX,QAAQ,CAACY,MAAK,GAAI,EAAE,EAAE;QAC/BR,MAAM,CAACS,KAAK,CAACN,WAAU,GAAI,IAAI;MACnC,OAAO;QACHH,MAAM,CAACS,KAAK,CAACN,WAAU,GAAI,KAAK;MACpC;MAEA,IAAII,QAAQ,CAACF,cAAa,IAAKE,QAAQ,CAACV,QAAQ,EAAE;QAC9CG,MAAM,CAACS,KAAK,CAACH,YAAW,GAAI,KAAK;MACrC,OAAO;QACHN,MAAM,CAACS,KAAK,CAACH,YAAW,GAAI,IAAI;MACpC;IAEJ,CAAC,EAAE;MAAEI,IAAI,EAAE;IAAK,CAAC,CAAC;IAGlB,OAAO;MACHlB,aAAa;MACbC,gBAAgB;MAChBC,QAAQ;MACRC,SAAS;MACTC,QAAQ;MACRC,QAAQ;MACRC,eAAe;MACfC,IAAI;MACJC;IACH,CAAC;EACN;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}