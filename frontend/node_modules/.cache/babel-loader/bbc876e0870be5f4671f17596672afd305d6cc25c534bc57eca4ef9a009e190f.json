{"ast":null,"code":"import Header from \"./components/header/HeaderCmp.vue\";\nimport Footer from \"./components/footer/FooterCmp.vue\";\nimport { useStore, mapGetters } from \"vuex\";\nimport { computed } from \"vue\";\nexport default {\n  name: \"App\",\n  components: {\n    Header,\n    Footer\n  },\n  setup() {\n    const store = useStore();\n\n    // Utilisation de mapGetters pour mapper les getters Vuex en tant que propriétés calculées\n    const {\n      getConnectionState,\n      getAccessToken,\n      getRefreshToken\n    } = mapGetters({\n      getConnectionState: 'getConnectionState',\n      getAccessToken: 'getAccessToken',\n      getRefreshToken: 'getRefreshToken'\n    });\n\n    // Utilisation de computed pour accéder aux valeurs des getters\n    const connectionState = computed(getConnectionState);\n    const accessToken = computed(getAccessToken);\n    const refreshToken = computed(getRefreshToken);\n\n    // Affichage des valeurs\n    console.log('connectionState', connectionState.value);\n    console.log('access token = ', accessToken.value);\n    console.log('refresh token = ', refreshToken.value);\n\n    // Exemple de modification de l'état Vuex à l'aide d'une mutation\n    store.commit('setConnectionState', true);\n    return {};\n  }\n};","map":{"version":3,"names":["Header","Footer","useStore","mapGetters","computed","name","components","setup","store","getConnectionState","getAccessToken","getRefreshToken","connectionState","accessToken","refreshToken","console","log","value","commit"],"sources":["/Users/chris/Desktop/matcha_42/frontend/src/App.vue"],"sourcesContent":["<template>\n  <div>\n    <Header></Header>\n    <router-view> </router-view>\n    <Footer></Footer>\n  </div>\n</template>\n\n<script>\nimport Header from \"./components/header/HeaderCmp.vue\";\nimport Footer from \"./components/footer/FooterCmp.vue\";\nimport { useStore, mapGetters } from \"vuex\";\nimport { computed } from \"vue\";\nexport default {\n  name: \"App\",\n  components: {\n    Header,\n    Footer,\n  },\n  \n  setup() {\n    const store = useStore();\n\n    // Utilisation de mapGetters pour mapper les getters Vuex en tant que propriétés calculées\n    const { getConnectionState, getAccessToken, getRefreshToken } = mapGetters({\n  getConnectionState: 'getConnectionState',\n  getAccessToken: 'getAccessToken',\n  getRefreshToken: 'getRefreshToken',\n});\n\n    // Utilisation de computed pour accéder aux valeurs des getters\n    const connectionState = computed(getConnectionState);\n    const accessToken = computed(getAccessToken);\n    const refreshToken = computed(getRefreshToken);\n\n    // Affichage des valeurs\n    console.log('connectionState', connectionState.value);\n    console.log('access token = ', accessToken.value);\n    console.log('refresh token = ', refreshToken.value);\n\n    // Exemple de modification de l'état Vuex à l'aide d'une mutation\n    store.commit('setConnectionState', true);\n\n    \n    return {\n\n    }\n    \n  },\n};\n</script>\n\n<style lang=\"scss\">\n@import url(\"./style/reset.css\");\n@import url(\"https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap\");\n\n:root {\n  --gray: rgb(103, 102, 102);\n  --light-gray: rgb(201, 200, 200);\n  --light-light-gray: rgba(219, 219, 219, 0.381);\n  --light-pink: #f790ce;\n  --pink: #f82ea7;\n  --purple: #9336cd;\n  --purple-placeholder-bg: #f1a3fcd7;\n  --dark-gray: rgb(71, 70, 70);\n}\n\n#app {\n  font-family: \"Roboto\", sans-serif;\n  /* padding: 0px 20px; */\n  background: url(../public/src/couple-bg.jpg) fixed center/cover;\n\n  height: 100vh;\n}\n\n.fade-blur-bg {\n  animation: fadeBlur 0.1s ease-in-out forwards;\n\n  @keyframes fadeBlur {\n    0% {\n      backdrop-filter: blur(0px);\n    }\n\n    100% {\n      backdrop-filter: blur(5px);\n    }\n  }\n}\n\n.hidden-element {\n  display: none !important;\n}\n\n.fade-In {\n  animation: fadeIn 0.5s ease forwards;\n