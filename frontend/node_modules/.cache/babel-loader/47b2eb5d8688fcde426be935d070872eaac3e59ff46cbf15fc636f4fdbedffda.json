eholder=\"$t('password')\">\n\n            <input v-model=\"inputs.repeatPassword\" :disabled=\"!inputs.emailValid\"\n                :class=\"{ 'text-red': !inputs.samePassword, 'text-green': inputs.samePassword }\" type=\"password\"\n                :placeholder=\"$t('passwordConfirm')\"> -->\n            <input v-model=\"inputs.password\" :disabled=\"!inputs.emailValid\"\n                :class=\"{ 'text-green': inputs.samePassword, 'disabled--input': !inputs.emailValid }\" type=\"password\"\n                :placeholder=\"$t('password')\">\n\n            <input v-model=\"inputs.repeatPassword\" :disabled=\"!inputs.emailValid\"\n                :class=\"{ 'text-red': !inputs.samePassword, 'text-green': inputs.samePassword, 'disabled--input': !inputs.emailValid }\"\n                type=\"password\" :placeholder=\"$t('passwordConfirm')\">\n\n\n\n            <div class=\"submit--btn\">\n                <button class=\"send--registration--btn\"\n                    :class=\"{ 'disabled': !inputs.samePassword || !inputs.emailValid || !inputs.userName || !inputs.firstName || !inputs.lastName }\">{{\n                        $t('send') }}</button>\n            </div>\n        </form>\n    </div>\n</template>\n\n<script>\nimport { useI18n } from \"vue-i18n\";\nimport { ref, watch } from 'vue';\n\n\nexport default {\n    name: \"RegisterForm\",\n\n    // methods: {\n\n    //     validateEmail(value) {\n            \n    //         return (/^\\w+([.-]?\\w+)*@\\w+([.-]?\\w+)*(.\\w{2,3})+$/.test(value));\n    //     }\n    // },\n    setup() {\n        // Traduction ----------------------------------\n\n\n\n\n        const { t } = useI18n();\n        const registerTitle = t(\"registerTitle\");\n        const registerSubTitle = t(\"registerSubTitle\");\n        const userName = t(\"userName\");\n        const firstName = t(\"firstName\");\n        const lastName = t(\"lastName\");\n        const password = t(\"password\");\n        const passwordConfirm = t(\"passwordConfirm\");\n        const send = t(\"send\");\n        // Input Object --------------------------------\n        let inputs = ref({\n\n            email: '',\n            emailValid: false,\n            password: '',\n            repeatPassword: '',\n            samePassword: false,\n        });\n\n        const maxLength = 15;\n\n        watch(inputs, (newValue) => {\n\n            if (newValue.email.length != 0) {\n\n                if (!validateEmail(newValue.email)) {\n                    inputs.value.emailValid = false;\n                    inputs.value.password = '';\n                    inputs.value.repeatPassword = '';\n                    inputs.value.samePassword = false;\n\n                } else {\n                    inputs.value.emailValid = true;\n                }\n            }\n            if (newValue.password.length != 0) {\n\n\n                if (newValue.repeatPassword != inputs.value.password) {\n                    inputs.value.samePassword = false;\n                } else {\n                    inputs.value.samePassword = true;\n                }\n\n            }\n\n\n\n        }, { deep: true });\n        methods: {\n\n        validateEmail(value) {\n            \n            return (/^\\w+([.-]?\\w+)*@\\w+([.-]?\\w+)*(.\\w{2,3})+$/.test(value));\n        }\n    }\n\n\n        return {\n            registerTitle,\n            registerSubTitle,\n            userName,\n            firstName,\n            lastName,\n            password,\n            passwordConfirm,\n            send,\n            inputs,\n            maxLength,\n            // validateEmail,\n        };\n    },\n}\n</script>\n\n<style lang=\"scss\">\n.register--container {\n    // border: solid 1px red;\n\n    // border: solid 1px red;\n    width: auto;\n    height: auto;\n    padding: 10px 35px 45px 35px;\n    border-radius: 15px;\n    background-image: linear-gradient(to right,\n            #ff24a7af,\n            #8890feb2);\n    box-shadow: 0 0 28px rgba(0, 0, 0, 0.498);\n    cursor: default;\n    user-select: none;\n\n    h3 {\n\n        margin: 10px 0 0 0;\n        color: white;\n        text-align: center;\n        font-weight: 900;\n        text-transform: capitalize;\n        font-size: calc(min(3vw + 2vh, 70px));\n        text-shadow: 0 0 18px rgba(255, 255, 255, 0.438);\n\n    }\n\n    #sub--title {\n        font-weight: 600;\n        font-size: calc(min(2vw + 1.6vh, 50px));\n        margin-bottom: 10px;\n    }\n\n    .register--form {\n        display: grid;\n        justify-items: center;\n\n        input {\n            padding: 6px;\n            margin: 10px;\n            width: 100%;\n            border: none;\n            outline: none;\n            border-radius: 8px;\n            text-align: center;\n            font-weight: 600;\n            // font-size: 1.2rem;  \n            background-color: var(--purple-placeholder-bg);\n            color: var(--purple);\n\n        }\n\n        input::placeholder {\n            color: white;\n            font-weight: 400;\n\n            /* Couleur du placeholder */\n        }\n\n        .submit--btn {\n            margin-top: 25px;\n\n            .send--registration--btn {\n\n                // padding: 10px 30px 10px 30px;\n\n                height: 45px;\n                width: 180px;\n                border-radius: 10px;\n                border: none;\n                color: white;\n                font-weight: 600;\n                font-size: 1.2rem;\n\n                transition: all 0.4s;\n                cursor: pointer;\n                box-shadow: 0 0 6px rgba(0, 0, 0, 0.381);\n\n                background-image: linear-gradient(to right,\n                        #ff24a7af,\n                        #8890feb2);\n\n                &:hover {\n                    background-image: linear-gradient(to right,\n                            #ff24a78a,\n                            #8890fe90);\n                    /* Dégradé de couleur */\n                    color: var(--purple);\n                    transform: scale(1.1);\n                }\n            }\n\n            .disabled {\n                opacity: 0.6;\n                cursor: default;\n\n                &:hover {\n                    background-image: linear-gradient(to right, #ff24a7af, #8890feb2);\n                    color: white;\n                    transform: none;\n                }\n            }\n        }\n\n        @media (min-width: 800px) {\n            width: 70%;\n            margin: auto;\n\n        }\n    }\n\n    .disabled--input {\n        opacity: 0.5;\n    }\n}\n\n// .disabled {\n//     /* Vos styles pour le bouton désactivé */\n//     opacity: 0.5; /* Par exemple, réduire l'opacité */\n//     cursor: none; /* Changer le curseur */\n// }</style>"],"mappings":";;EACSA,KAAK,EAAC;AAAsB;;EAEzBC,EAAE,EAAC;AAAY;;EACbC,MAAM,EAAC,EAAE;EAACF,KAAK,EAAC;;mBAJ9B;mBAAA;mBAAA;mBAAA;mBAAA;;EA6BiBA,KAAK,EAAC;AAAa;;uBA5BhCG,mBAAA,CAkCM,OAlCNC,UAkCM,GAjCFC,mBAAA,CAAkC,YAAAC,gBAAA,CAA3BC,IAAA,CAAAC,EAAE,mCACTH,mBAAA,CAAqD,MAArDI,UAAqD,EAAAH,gBAAA,CAA9BC,IAAA,CAAAC,EAAE,sCACzBH,mBAAA,CA8BO,QA9BPK,UA8BO,G,gBA7BHL,mBAAA,CAAkG;IAL9G,uBAAAM,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAK4BL,IAAA,CAAAM,MAAM,CAACC,QAAQ,GAAAF,MAAA;IAAGG,SAAS,EAAER,IAAA,CAAAS,SAAS;IAAEC,IAAI,EAAC,MAAM;IAAEC,WAAW,EAAEX,IAAA,CAAAC,EAAE;0BALhGW,UAAA,I,cAK4BZ,IAAA,CAAAM,MAAM,CAACC,QAAQ,E,mBAC/BT,mBAAA,CAAoG;IANhH,uBAAAM,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAM4BL,IAAA,CAAAM,MAAM,CAACO,SAAS,GAAAR,MAAA;IAAGG,SAAS,EAAER,IAAA,CAAAS,SAAS;IAAEC,IAAI,EAAC,MAAM;IAAEC,WAAW,EAAEX,IAAA,CAAAC,EAAE;0BANjGa,UAAA,I,cAM4Bd,IAAA,CAAAM,MAAM,CAACO,SAAS,E,mBAChCf,mBAAA,CAAkG;IAP9G,uBAAAM,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAO4BL,IAAA,CAAAM,MAAM,CAACS,QAAQ,GAAAV,MAAA;IAAGG,SAAS,EAAER,IAAA,CAAAS,SAAS;IAAEC,IAAI,EAAC,MAAM;IAAEC,WAAW,EAAEX,IAAA,CAAAC,EAAE;0BAPhGe,UAAA,I,cAO4BhB,IAAA,CAAAM,MAAM,CAACS,QAAQ,E,mBAE/BjB,mBAAA,CACqG;IAVjH,uBAAAM,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAS4BL,IAAA,CAAAM,MAAM,CAACW,KAAK,GAAAZ,MAAA;IAAEK,IAAI,EAAC,OAAO;IAACC,WAAW,EAAC,QAAQ;IAC1DlB,KAAK,EAVtBy